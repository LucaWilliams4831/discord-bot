{
  "name": "gearbot",
  "version": "0.0.1",
  "description": "Gearbox discord bot",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist/src/forever.js",
    "dev": "env-cmd -f .env.dev ts-node-dev --respawn --transpile-only src/index.ts | pino-pretty -c",
    "test": "env-cmd -f .env.dev jest --coverage",
    "test:watch": "env-cmd -f .env.dev jest --watch",
    "lint:ts": "eslint . --ext .tsx,.ts",
    "format:ts": "eslint . --ext .tsx,.ts --fix",
    "csv": "ts-node node-scripts/transform-users.ts",
    "csv:test": "env-cmd -f .env.dev ts-node node-scripts/test-users.ts | pino-pretty -c",
    "migrate:get-all-users": "env-cmd -f .env.dev ts-node node-scripts/get-all-users.ts | pino-pretty -c",
    "migrate:merge-lists": "ts-node node-scripts/merge-users.ts",
    "migrate:update-users": "env-cmd -f .env.dev ts-node node-scripts/change-discord-id.ts | pino-pretty -c"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.2",
    "fast-csv": "^4.3.6",
    "forever-monitor": "^3.0.3",
    "pg": "^8.7.1",
    "pino": "^7.6.0"
  },
  "engines": {
    "node": ">=16.x"
  },
  "devDependencies": {
    "@types/forever-monitor": "^1.7.6",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "@types/pg": "^8.6.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "jest-mock-extended": "^2.0.4",
    "pino-pretty": "^7.3.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.0"
  }
}
